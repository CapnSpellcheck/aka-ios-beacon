source 'https://github.com/artsy/Specs.git'
source 'https://github.com/CocoaPods/Specs.git'
source 'https://sources.aka-labs.com/scm/git/cocoapods'
workspace 'AKAControls.xcworkspace'
xcodeproj 'AKAControls.xcodeproj'
xcodeproj 'AKAControlsDemo/AKAControlsDemo.xcodeproj'

target :AKAControlsDemo do
    platform :ios, '8.0'
    pod 'AKACommons', :path => '..'
    pod 'AKAControls', :path => '..'
    xcodeproj 'AKAControlsDemo/AKAControlsDemo.xcodeproj'
end

target :AKAControls do
    platform :ios, '8.0'
    pod 'AKACommons', :path => '..'
    xcodeproj 'AKAControls.xcodeproj'
end

